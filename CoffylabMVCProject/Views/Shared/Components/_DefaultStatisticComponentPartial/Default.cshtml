<div class="row counter-row">
	<div class="col-lg-3 col-md-6 single-counter">
		<h1 class="counter">@ViewBag.ProductCount</h1>
		<p>Product</p>
	</div>
	<div class="col-lg-3 col-md-6 single-counter">
		<h1 class="counter">@ViewBag.CategoryCount</h1>
		<p>Category</p>
	</div>
	<div class="col-lg-3 col-md-6 single-counter">
		<h1 class="counter">@ViewBag.TestimonialCount</h1>
		<p>Testimonial</p>
	</div>
	<div class="col-lg-3 col-md-6 single-counter">
		<h1 class="counter">@ViewBag.SubscriberCount</h1>
		<p>Subscriber</p>
	</div>
</div>